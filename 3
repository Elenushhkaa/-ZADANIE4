#include <iostream>

using namespace std;

int main()
{
	srand(time(NULL));
	setlocale(LC_ALL, "RUS");
	cout << "ЗАДАЙТЕ МАССИВ" <<endl;
	int n, Elenushka, x;
	cin >> n;

	int* MAS = new int[n];
	Elenushka = 0;

	for (int i = 0; i < n; ++i) {
		x = rand() % 1000 + 1000;
		MAS[i] = x;
		if (x / 10 % 2 == 0)
			Elenushka++;



	}

	cout << Elenushka << ' ';
	delete MAS;
	return 0;
}
